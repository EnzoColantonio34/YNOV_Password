<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:views="clr-namespace:YNOV_Password.Views"
        xmlns:vm="clr-namespace:YNOV_Password.ViewModels"
        mc:Ignorable="d"
        x:Class="YNOV_Password.Views.AddPasswordWindow"
        x:DataType="vm:AddPasswordViewModel"
        Title="{Binding WindowTitle}"
        Width="400"
        Height="300"
        WindowStartupLocation="CenterOwner">

    <Window.Styles>
        <StyleInclude Source="avares://YNOV_Password/Styles/main.axaml"/>
    </Window.Styles>

    <views:AddPasswordView />
</Window>
